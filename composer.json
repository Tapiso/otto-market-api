{
  "name": "creatissimo/otto-market-api",
  "description": "PHP library for OTTO Market API build from swagger schema",
  "keywords": [
    "openapitools",
    "openapi-generator",
    "openapi",
    "php",
    "sdk",
    "rest",
    "api"
  ],
  "homepage": "https://github.com/creatissimo/otto-market-api",
  "license": "MIT",
  "require": {
    "php": "^7.4 || ^8.0",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "guzzlehttp/guzzle": "^7.3",
    "guzzlehttp/psr7": "^1.7 || ^2.0"
  },
  "autoload": {
    "psr-4": {
      "Otto\\Client\\": "lib/"
    }
  }
}
